{
    "name": "@qcksys/croncheck",
    "version": "0.0.2",
    "description": "Modern library for parsing and matching cron expressions",
    "keywords": [
        "cron"
    ],
    "homepage": "https://github.com/qcksys/croncheck#readme",
    "bugs": {
        "url": "https://github.com/qcksys/croncheck/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/qcksys/croncheck.git"
    },
    "license": "MIT",
    "author": "Tom Alle <tom@qcksys.com>",
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "scripts": {
        "test": "vitest --run",
        "test:watch": "vitest",
        "build": "tsdown",
        "prepublish": "tsdown",
        "biome:check:unsafe": "biome check --write --unsafe",
        "biome:ci": "biome ci",
        "prepare": "husky",
        "precommit": "pnpm run biome:check:unsafe && tsc && pnpm run test"
    },
    "dependencies": {
        "@date-fns/tz": "^1.4.1",
        "date-fns": "^4.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.7",
        "husky": "^9.1.7",
        "tsdown": "^0.16.8",
        "typescript": "^5.9.3",
        "vitest": "^4.0.14"
    },
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.cts"
}
